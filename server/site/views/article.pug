//- postID, postUUID, userID, username, fullName, postTime, description
each val in input
    div.article(postUUID=val.postUUID)
        a.head.account(href=`user/${val.username}`)
            div.user
                img(src='assets/avatar.jpg')
                p= val.fullName
        img.post(src=`images/${val.postUUID}.jpg`)
        div.conversation
            p= val.description
